{
  "manifest_version": 3,
  "name": "Add a Gif in Any Class Extension",
  "version": "2.0.1",
  "description": "A Chrome extension that adds a GIF to all classes on Every page that you want.",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}